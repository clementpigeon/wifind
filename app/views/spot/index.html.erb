    ================================================== -->

	<link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css">
  
	  <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
	  <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>

	  <script>

    console.log(<%= raw @hash.to_json%>)


	  $(function() {
	    $( "#price_slider" ).slider({
	      value:0,
	      min: 0,
	      max: 500,
	      step: 50,
		  orientation: "horizontal",
	      slide: function( event, ui ) {
	        $( "#price_label" ).val( "$" + ui.value );
	      }
	    });
	    $( "price_label" ).val( "$" + $( "#price_slider" ).slider( "value" ) );
		
	    $( "#wifi_slider" ).slider({
	      value:0,
	      min: 0,
	      max: 500,
	      step: 50,
		  orientation: "horizontal",
	      slide: function( event, ui ) {
	        $( "wifi_label" ).val( "$" + ui.value );
	      }
	    });
	    $( "#wifi_label" ).val( "$" + $( "#wifi_slider" ).slider( "value" ) );
		
		
	    $( "#noise_slider" ).slider({
	      value:0
		  ,
	      min: 0,
	      max: 500,
	      step: 50,
		  orientation: "horizontal",
	      slide: function( event, ui ) {
	        $( "#noise_label" ).val( "$" + ui.value );
	      }
	    });
	    $( "#noise_label" ).val( "$" + $( "#noise_slider" ).slider( "value" ) );
		
	  });
	  </script>
